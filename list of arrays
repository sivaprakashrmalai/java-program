  for (String i:a) {
  System.out.println(i); 
  }
  System.out.println(a[0] + " CSK team captain and also good keeper");
  
  System.out.println(a[1] + " is a best bowler and also good batsman" );
  
  System.out.println(a[3] + " is a opener batsman" );
  
  System.out.println(a[6] + " is a best bowler" );
  
  System.out.println(a[7] + " is a best bats man" );
  
